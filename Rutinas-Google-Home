metadata:
  name: Pon mi musica
  description: rutina para reproducir lista Spotify de hyundai
automations:
  - starters:
      - type: assistant.event.OkGoogle
        eventData: query
        is: Pon mi musica
    actions:
      - type: device.command.SetVolume
        volumeLevel: 40
        devices:
          - Parlante - Bocinas
      - type: device.command.SetVolume
        volumeLevel: 50
        devices:
          - Googolina - Bocinas
      - type: assistant.command.OkGoogle
        okGoogle: Reproducir mi playlist Hyundai aleatorio en parlanchinas
        devices:
          - Parlante - Bocinas
